{
  "name": "crabcakes",
  "version": "1.0.0",
  "description": "S3-compatible server written in Rust",
  "type": "module",
  "packageManager": "pnpm@10.18.1",
  "scripts": {
    "build-ts": "swc src/js --delete-dir-on-start --strip-leading-paths --out-dir target/js/ *.ts",
    "build": "pnpm build-ts && node build-js.js",
    "lint:css": "stylelint 'static/css/**/*.css'",
    "lint": "pnpm run lint:css"
  },
  "devDependencies": {
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "@testing-library/dom": "^10.4.1",
    "esbuild": "^0.25.10",
    "eslint": "^9.37.0",
    "jsdom": "^27.0.0",
    "stylelint": "^16.25.0",
    "stylelint-config-standard": "^39.0.1",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.906.0"
  }
}